		"rehype-highlight": "^7.0.1",
		"rehype-katex": "^7.0.1",
		"rehype-mathjax": "^6.0.0",
		"rehype-pretty-code": "^0.14.0",
		"rehype-raw": "^7.0.0",
		"rehype-sanitize": "^6.0.0",
		"rehype-stringify": "^10.0.1",
		"remark": "^15.0.1",
		"remark-breaks": "^4.0.0",
		"remark-gfm": "^4.0.0",
		"remark-html": "^16.0.1",
